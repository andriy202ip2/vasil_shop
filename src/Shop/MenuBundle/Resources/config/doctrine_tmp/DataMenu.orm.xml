<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Shop\MenuBundle\Entity\DataMenu" table="data_menu">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="modelMenuId" type="integer" column="model_menu_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="autoMenuId" type="integer" column="auto_menu_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
